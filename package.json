{
  "name": "memo",
  "version": "1.1.1",
  "description": "A memo for myself",
  "keywords": [
    "blog"
  ],
  "homepage": "https://github.com/orzyyyy/memo#readme",
  "bugs": {
    "url": "https://github.com/orzyyyy/memo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orzyyyy/memo"
  },
  "license": "MIT",
  "author": "orzyyyy",
  "scripts": {
    "build": "rimraf dist && cross-env BUILD_ENV=netlify nino koei",
    "build:analyse": "rimraf dist && cross-env BUILD_ENV=analyse nino koei",
    "build:business": "rimraf dist && cross-env BUILD_ENV=business nino koei",
    "build:gh-pages": "rimraf dist && cross-env BUILD_ENV=gh-pages nino koei",
    "clean": "rimraf node_modules package-lock.json bin dist && npm i",
    "codecov": "nino test --codecov",
    "compile:server": "rimraf bin && tsc -p server",
    "d": "npm run compile:server -- --watch",
    "deploy": "npm-run-all clean build:gh-pages pub",
    "dev": "rimraf dist && cross-env BUILD_ENV=dev nino koei -w -d",
    "dev:business": "rimraf dist && cross-env BUILD_ENV=business-dev nino koei -w -d",
    "lint": "nino eslint",
    "lint-staged": "lint-staged",
    "prettier": "nino prettier",
    "pub": "gh-pages -d dist",
    "run:server": "node bin",
    "server": "npm-run-all compile:server run:server",
    "sort": "npx sort-package-json",
    "start": "npm-run-all --parallel server dev",
    "start:business": "npm-run-all --parallel server dev:business",
    "test": "rimraf bin dist && nino test"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@ant-design/icons": "^4.0.0-alpha.8",
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.30",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/blueimp-md5": "^2.7.0",
    "@types/enzyme": "^3.10.3",
    "@types/fetch-mock": "^7.3.1",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.22",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/marked": "^0.7.0",
    "@types/mysql": "^2.15.7",
    "@types/prettier": "^1.18.3",
    "@types/puppeteer-core": "^1.9.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-lazyload": "^2.6.0",
    "@types/react-router-dom": "^5.1.2",
    "@types/request": "^2.48.3",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.8",
    "antd": "^4.0.0-alpha.2",
    "blueimp-md5": "^2.12.0",
    "chalk": "^2.4.2",
    "core-js": "^3.3.6",
    "cross-env": "^6.0.3",
    "date-fns": "^2.6.0",
    "fetch-mock": "^7.7.3",
    "fs-extra": "^8.1.0",
    "gh-pages": "^2.1.1",
    "github-markdown-css": "^3.0.1",
    "history": "^4.10.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lint-staged": "^9.4.2",
    "marked": "^0.7.0",
    "mini-xmind": "^1.4.1",
    "mockdate": "^2.0.5",
    "moment-locales-webpack-plugin": "^1.1.0",
    "mysql": "^2.17.1",
    "nino-cli": "^1.3.6",
    "npm-run-all": "^4.1.5",
    "post-compile-webpack-plugin": "^0.1.2",
    "puppeteer-core": "^2.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-lazyload": "^2.6.2",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "supertest": "^4.0.2",
    "toml": "^3.0.0",
