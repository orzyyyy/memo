{
  "name": "memo",
  "version": "1.0.1",
  "description": "A memo for myself",
  "keywords": [
    "blog"
  ],
  "homepage": "https://github.com/orzyyyy/memo#readme",
  "bugs": {
    "url": "https://github.com/orzyyyy/memo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orzyyyy/memo"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run pre:css && tsc && cross-env BUILD_ENV=prod nino koei -c scripts/build.js",
    "clean": "rimraf node_modules package-lock.json bin dist && npm i",
    "codecov": "nino test --codecov",
    "compile:server": "tsc -p server",
    "d": "npm run compile:server -- --watch",
    "deploy": "node scripts/publish",
    "dev": "npm run pre:css && nino koei -c scripts/build.js -w -d",
    "generate": "node scripts/generateSider",
    "lint": "tslint -c tslint.json 'src/**/*.tsx' 'server/**/*.ts'",
    "lint-staged": "lint-staged",
    "pre:css": "rimraf dist && tsc && cp -r src/pages/css dist/src/pages",
    "pre:start": "npm-run-all compile:server clean:views generate",
    "prettier": "nino prettier",
    "run:server": "node bin",
    "server": "npm-run-all compile:server run:server",
    "sort": "npx sort-package-json",
    "start": "npm-run-all --parallel server dev",
    "test": "nino test",
    "tsc": "tsc"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^4.0.0-alpha.2",
    "core-js": "^3.2.1",
    "koa": "^2.8.1",
    "mini-xmind": "^1.3.0",
    "nino-cli": "^1.0.3",
    "puppeteer-core": "^1.19.0",
    "react-lazyload": "^2.6.2",
    "react-markdown": "^4.1.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.0.0-alpha.8",
    "@types/blueimp-md5": "^2.7.0",
    "@types/fs-extra": "^8.0.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/prettier": "^1.18.2",
    "@types/puppeteer-core": "^1.9.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@types/react-lazyload": "^2.5.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/request-promise": "^4.1.44",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.8",
    "blueimp-md5": "^2.11.1",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "date-fns": "^2.0.0-beta.2",
    "fs-extra": "^8.1.0",
    "gh-pages": "^2.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lint-staged": "^9.2.3",
    "mockdate": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "post-compile-webpack-plugin": "^0.1.2",
    "react-router-dom": "^5.0.1",
    "request-promise": "^4.2.4",
    "rimraf": "^3.0.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "supertest": "^4.0.2",
    "toml": "^3.0.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "walk": "^2.3.14",
    "webpack-bundle-analyzer": "^3.4.1"
  }
}
