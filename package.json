{
  "name": "memo",
  "version": "1.1.0",
  "description": "A memo for myself",
  "keywords": [
    "blog"
  ],
  "homepage": "https://github.com/orzyyyy/memo#readme",
  "bugs": {
    "url": "https://github.com/orzyyyy/memo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orzyyyy/memo"
  },
  "license": "MIT",
  "scripts": {
    "analyse": "rimraf dist && cross-env BUILD_ENV=analyse nino koei --not-copy-assets",
    "build": "rimraf dist && cross-env BUILD_ENV=prod nino koei --not-copy-assets",
    "clean": "rimraf node_modules package-lock.json bin dist && npm i",
    "codecov": "nino test --codecov",
    "compile:server": "tsc -p server",
    "d": "npm run compile:server -- --watch",
    "dev": "rimraf dist && nino koei -w -d --not-copy-assets",
    "lint": "nino eslint",
    "lint-staged": "lint-staged",
    "prettier": "nino prettier",
    "run:server": "node bin",
    "server": "npm-run-all compile:server run:server",
    "sort": "npx sort-package-json",
    "start": "npm-run-all --parallel server dev",
    "test": "nino test"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0-alpha.8",
    "antd": "^4.0.0-alpha.2",
    "core-js": "^3.2.1",
    "koa": "^2.8.1",
    "mini-xmind": "^1.3.2",
    "mysql": "^2.17.1",
    "puppeteer-core": "^1.20.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@types/blueimp-md5": "^2.7.0",
    "@types/fs-extra": "^8.0.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/marked": "^0.6.5",
    "@types/mysql": "^2.15.7",
    "@types/prettier": "^1.18.2",
    "@types/puppeteer-core": "^1.9.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-lazyload": "^2.5.0",
    "@types/request": "^2.48.3",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.8",
    "blueimp-md5": "^2.12.0",
    "chalk": "^2.4.2",
    "cross-env": "^6.0.0",
    "date-fns": "^2.2.1",
    "fs-extra": "^8.1.0",
    "gh-pages": "^2.1.1",
    "github-markdown-css": "^3.0.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lint-staged": "^9.2.5",
    "marked": "^0.7.0",
    "mockdate": "^2.0.5",
    "moment-locales-webpack-plugin": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "post-compile-webpack-plugin": "^0.1.2",
    "react-lazyload": "^2.6.2",
    "rimraf": "^3.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "supertest": "^4.0.2",
    "toml": "^3.0.0",
    "walk": "^2.3.14",
    "webpack-bundle-analyzer": "^3.5.1"
  }
}
