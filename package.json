{
  "name": "memo",
  "version": "1.1.4",
  "description": "A memo for myself",
  "keywords": [
    "blog"
  ],
  "homepage": "https://github.com/orzyyyy/memo#readme",
  "bugs": {
    "url": "https://github.com/orzyyyy/memo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orzyyyy/memo"
  },
  "license": "MIT",
  "author": "orzyyyy",
  "scripts": {
    "build": "cross-env BUILD_ENV=netlify npm run build:common",
    "build:analyse": "cross-env BUILD_ENV=analyse npm run build:common",
    "build:common": "npm-run-all build:prepare build:content build:completed",
    "build:completed": "rimraf dist && cp -r _dist dist && rimraf _dist",
    "build:content": "nino koei --output=_dist",
    "build:gh-pages": "cross-env BUILD_ENV=gh-pages npm run build:common",
    "build:prepare": "rimraf _dist",
    "build:private": "cross-env BUILD_ENV=private npm run build:common",
    "clean": "rimraf node_modules _dist",
    "codecov": "cross-env BUILD_ENV=test nino test --codecov",
    "compile:server": "npm run pre:server && tsc -p server",
    "cross-env": "cross-env",
    "d": "npm run compile:server -- --watch",
    "dev": "rimraf dist && nino koei -w -d",
    "go": "pm2 start",
    "lint": "nino eslint",
    "lint-staged": "lint-staged",
    "log": "cross-var git log --graph --pretty=format:\"[%h](http://github.com/orzyyyy/memo/commit/%H) -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset\" --abbrev-commit --date=relative $npm_package_version..HEAD",
    "pre:server": "rimraf bin && mkdir bin && cpr server/resource bin/resource && sh ./scripts/clean-proxy.sh",
    "prettier": "nino prettier",
    "restart": "pm2 restart memo-server",
    "run:server": "node bin",
    "sort": "npx sort-package-json",
    "start": "npm run compile:server && wait-on bin && cross-env BUILD_ENV=dev npm-run-all --parallel run:server dev",
    "test": "cross-env BUILD_ENV=test nino test",
    "tsc": "tsc"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier",
      "npm run lint"
    ],
    "package.json": [
      "npm run sort"
    ]
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.1",
    "fs-extra": "^10.1.0",
    "github-markdown-css": "^5.1.0",
    "history": "^5.3.0",
    "marked": "^4.1.0",
    "mini-xmind": "^1.4.2",
    "nino-cli": "^1.4.6",
    "npm-run-all": "^4.1.5",
    "post-compile-webpack-plugin": "^0.1.2",
    "rc-dialog": "^8.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.1.2",
    "request": "^2.88.2",
    "socket.io-client": "^4.5.3",
    "toml": "^3.0.0",
    "typescript": "^5.0.4",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "devDependencies": {
    "@koa/router": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__template": "^7.4.1",
    "@types/blueimp-md5": "^2.18.0",
    "@types/body-parser": "^1.19.2",
    "@types/caseless": "^0.12.2",
    "@types/cheerio": "^0.22.31",
    "@types/enzyme": "^3.10.12",
    "@types/eslint": "^8.37.0",
    "@types/fetch-mock": "^7.3.5",
    "@types/fs-extra": "^9.0.13",
    "@types/istanbul-lib-report": "^3.0.0",
    "@types/jest": "^29.0.0",
    "@types/koa": "^2.13.6",
    "@types/koa__router": "^8.0.11",
    "@types/koa-router": "^7.4.4",
    "@types/koa-send": "^4.1.3",
    "@types/koa-static": "^4.0.2",
    "@types/marked": "^4.0.6",
    "@types/mime": "^3.0.1",
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.0",
    "@types/prop-types": "^15.7.5",
    "@types/qs": "^6.9.7",
    "@types/range-parser": "^1.2.4",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/request": "^2.48.8",
    "@types/scheduler": "^0.16.3",
    "@types/tough-cookie": "^4.0.2",
    "@types/yargs-parser": "^21.0.0",
    "blueimp-md5": "^2.19.0",
    "cpr": "^3.0.1",
    "cross-var": "^1.1.0",
    "fetch-mock": "^9.11.0",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-static": "^5.0.0",
    "lint-staged": "^13.0.3",
    "log4js": "^6.6.1",
    "mockdate": "^3.0.5",
    "node-fetch": "^2.6.7",
    "open": "^8.4.0",
    "pm2": "^5.2.0",
    "rimraf": "^3.0.2",
    "socket.io": "^4.5.3",
    "wait-on": "^7.0.1"
  }
}
