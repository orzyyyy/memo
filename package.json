{
  "name": "memo",
  "version": "0.0.0",
  "description": "A memo for myself",
  "scripts": {
    "start": "bash ./scripts/start-all.sh",
    "lint": "tslint -c tslint.json 'src/**/*.tsx' 'server/**/*.ts'",
    "server": "tsc && node server",
    "kill": "taskkill /f /im node.exe",
    "dev": "nino koei -c scripts/build -w -d -t",
    "build": "tsc && npm run generate && nino koei -c scripts/build",
    "prettier": "nino prettier",
    "test": "nino test",
    "codecov": "nino test -d",
    "lint-staged": "lint-staged",
    "deploy": "node scripts/publish",
    "generate": "node scripts/generateSider",
    "d": "npm run build && node scripts/publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zy410419243/memo"
  },
  "keywords": [
    "blog"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zy410419243/memo/issues"
  },
  "homepage": "https://github.com/zy410419243/memo#readme",
  "dependencies": {
    "antd": "^3.19.3",
    "koa": "^2.7.0",
    "mini-xmind": "^1.2.2",
    "nino-cli": "^0.8.2",
    "puppeteer-core": "^1.17.0",
    "react-markdown": "^4.0.8",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/blueimp-md5": "^2.7.0",
    "@types/fs-extra": "^7.0.0",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "blueimp-md5": "^2.10.0",
    "chalk": "^2.4.2",
    "date-fns": "^2.0.0-alpha.33",
    "gh-pages": "^2.0.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lint-staged": "^8.2.1",
    "react-router-dom": "^5.0.1",
    "toml": "^3.0.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  }
}
