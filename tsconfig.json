{
  "compilerOptions": {
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "target": "es6",
    "lib": ["dom", "es7"],
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "module": "commonjs"
  },
  "exclude": ["dist", "**/__tests__/**"],
  "typings": "./typings.d.ts"
}
